Phase:	UrlPre
Input:  Token SpaceToken
Options: control = appelt

Rule: Urlpre

(	

   ( ({Token.string == "http"}|{Token.string == "https"}|{Token.string == "ftp"})
	 {Token.string == ":"}{Token.string == "/"}{Token.string == "/"}{Token.string == "www"}{Token.string == "."}
    )
|
	(({Token.string == "http"}	| {Token.string == "https"}
	  {Token.string == "ftp"})
	 {Token.string == ":"}
	 {Token.string == "/"}
         {Token.string == "/"}
    )	|
	({Token.string == "www"}
         {Token.string == "."}
        )
):urlpre
-->
:urlpre.UrlPre = {rule = "UrlPre"}


