				                        ]
                      				) .



ex:somePlant  a        ex:Plant ;
        rdfs:label     "Plant Nevada" ;
        ex:hasFactory  ex:someFactory .

ex:someFactory  a      lgdo:Factory ;
        rdfs:label     "Gigafactory" ;
        ex:hasPolygon  ex:somePolygon ;
